package com.turmab.helpdesk;

import org.slf4j.Logger;
import org.slf4j.LoggerFactory;
import org.springframework.boot.SpringApplication;
import org.springframework.boot.autoconfigure.SpringBootApplication;

@SpringBootApplication
public class HelpdeskturmabApplication {

    private static final Logger log = LoggerFactory.getLogger(HelpdeskturmabApplication.class);

    public static void main(String[] args) {
        SpringApplication app = new SpringApplication(HelpdeskturmabApplication.class);   
    }
    
    @Override
    public void run(String... args) throws Exception {
        log.info("Aplicação helpdesk iniciada com sucesso");
    }
}